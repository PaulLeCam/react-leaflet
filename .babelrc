{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-transform-proto-to-assign",
    "@babel/plugin-transform-strict-mode",
    "dev-expression",
    "lodash"
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current",
              "browsers": ["last 2 versions", "ie >= 11"]
            },
            "modules": "commonjs",
            "loose": true
          }
        ],
        "@babel/preset-flow",
        "@babel/preset-react"
      ],
      "plugins": ["@babel/plugin-transform-runtime"]
    },
    "rollup": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current",
              "browsers": ["last 2 versions", "ie >= 11"]
            },
            "modules": false
          }
        ],
        "@babel/preset-flow",
        "@babel/preset-react"
      ],
      "plugins": [
        "./build/use-lodash-es",
        "@babel/plugin-transform-runtime"
      ]
    },
    "jsnext": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current",
              "browsers": ["last 2 versions", "ie >= 11"]
            },
            "modules": false,
            "loose": true
          }
        ],
        "@babel/preset-flow",
        "@babel/preset-react"
      ],
      "plugins": [
        "./build/use-lodash-es",
        ["@babel/plugin-transform-runtime", { "useESModules": true }]
      ]
    }
  }
}
