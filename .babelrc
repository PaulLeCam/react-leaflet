{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-transform-proto-to-assign",
    "@babel/plugin-transform-strict-mode",
    "dev-expression"
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current",
              "browsers": ["last 2 versions", "ie >= 11"]
            },
            "modules": "commonjs",
            "loose": true
          }
        ],
        "@babel/preset-flow",
        "@babel/preset-react"
      ],
      "plugins": [["@babel/plugin-transform-runtime", { "version": "^7.12.1" }]]
    },
    "rollup": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current",
              "browsers": ["last 2 versions", "ie >= 11"]
            },
            "modules": false
          }
        ],
        "@babel/preset-flow",
        "@babel/preset-react"
      ],
      "plugins": [["@babel/plugin-transform-runtime", { "version": "^7.12.1" }]]
    },
    "jsnext": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current",
              "browsers": ["last 2 versions", "ie >= 11"]
            },
            "modules": false,
            "loose": true
          }
        ],
        "@babel/preset-flow",
        "@babel/preset-react"
      ],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          { "useESModules": true, "version": "^7.12.1" }
        ]
      ]
    }
  }
}
